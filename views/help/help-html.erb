<div class="help-wrapper">
	<div class="help">
		<div class="help-tab">
			<div class="more">
				MORE
			</div>
		</div>
		<div class="help-main-wrap">
			<div class="game-cover-wrap">
				<%= erb "help/images/#{game}".to_sym %>
			</div>
			<div class="help-main">
				<%= erb "help/descriptions/#{game}".to_sym %>
			</div>
			<div class="links">
				<div class="link">
					<a href="/" title="Treintipollo">
						<img alt="Treintipollo" src="/assets/apple-touch-icon.png"></img>
					</a>
				</div>
				<div class="fb link">
					<a href="#" title="Share on Facebook">
						<img alt="Share on Facebook" src="/assets/fb.png"></img>
					</a>
				</div>
				<div class="tw link">
					<a href="#" title="Share on Twitter">
						<img alt="Share on Twitter" src="/assets/tw.png"></img>
					</a>
				</div>
			</div>
		</div>
	</div>
</div>